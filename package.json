{
  "name": "acceleott",
  "version": "1.0.0",
  "private": true,
  "description": "Unified deployment for Acceleott frontend (React) and backend (Express + MongoDB).",
  "scripts": {
    "postinstall": "cd acceleott-mueller && npm install",
    "dev": "cd acceleott-mueller && npm run dev",
    "build": "cd acceleott-mueller && npm run build",
    "preview": "cd acceleott-mueller && npm run preview",
    "start": "node acceleott-backend/server.js",
    "backend": "nodemon acceleott-backend/server.js"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "mongoose": "^8.19.2",
    "react-router-dom": "^7.8.2",
    "serverless-http": "^4.0.0"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "vite": "^7.1.12",
    "nodemon": "^3.1.7"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT"
}
