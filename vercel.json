{
"version": 2,
"builds": [
{
"src": "acceleott-mueller/package.json",
"use": "@vercel/node",
"config": {
"buildCommand": "npm install --prefix acceleott-mueller && npm run --prefix acceleott-mueller build"
}
},
{
"src": "acceleott-backend/server.js",
"use": "@vercel/node"
}
],
"rewrites": [
{
"source": "/api/(.)", 
"destination": "/acceleott-backend/server.js"
},
{
"source": "/",
"destination": "/acceleott-mueller/dist/index.html"
},
{
"source": "/(.)", 
"destination": "/acceleott-mueller/dist/$1"
}
]
}